[
  {
    "kernelSource": {
      "name": "xaga-android15",
      "repo": "https://github.com/zhang-ruixuan/xiaomi_xaga_kernel",
      "branch": "mglru",
      "device": "xaga",
      "defconfig": "xaga_defconfig"
    },
    "toolchains": [
      {
        "name": "clang",
        "repo": "https://gitlab.com/ThankYouMario/android_prebuilts_clang-standalone/",
        "branch": "14",
        "binaryEnv": ["bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "CONFIG_THINLTO": "y"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/easterNday/AnyKernel3/",
        "branch": "thyme"
      }
    },
    "enableKernelSU": true,
    "enableLXC": true
  }
]
